<h1 class="text-center titulo-Narajanja">Universidad <strong>Autonoma del caribe</strong></h1>
<div class="container_session">
  <div class="session">
    <center><h2 class="titulo-Narajanja">Registrarse</h2></center>
    <%= simple_form_for(resource, as: resource_name, id: "signupForm" , url: registration_path(resource_name)) do |f| %>
        <%= f.error_notification %>
        <p>Debe iniciar sesión o registrarse antes de continuar.<p>
        <%= f.input :username, id:"nombre", name:"nombre" %>
        <%= f.input :email, placeholder:"Correo", id:"email", name:"email" %>
        <%= f.association :programa, collection: Programa.order(:nombre) ,label_method: :nombre, input_html: {class: "select2select", "data-placeholder": "Seleccione Programa"} %>
        <%= f.input :password,  placeholder:"Contraseña" %>
        <%= f.input :password_confirmation, placeholder:"Confirme contraseña" %>
        <%= f.button :submit, "Crear usuario", class: "btn-primary btn-block Veamos" %>
        <a class="btn btn-sm btn-white btn-block Veamos" href="<%=new_user_session_path%>">Iniciar Seccion</a>
    <% end %>
  </div>
</div>
