<div class="container_session">
  <div class="session">
    <%= simple_form_for @programa, html: { class: 'form-horizontal' } do |f| %>
      <%= f.input :nombre , autofocus: true, placeholder:"Nombre De la programa" , autocomplete: "nombre",require:true %>
      <%= f.association :facultad, label_method: :nombre, value_method: :id , require:true, prompt: 'Selecione Tipo De Facultad' , input_html: {min:"1"}  %>
      <%= f.button :submit, "Registrar Programa",  class: "btn-primary block full-width m-b" %>
    <% end %>
  </div>
</div>
